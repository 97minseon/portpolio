<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cook.mymealkit.mapper.DeliveryMapper">
	
	<select id="delist" resultType="com.cook.mymealkit.domain.DeliveryVO">
		select * from delivery
	</select>
	
	<select id="delistByMemberId" resultType="com.cook.mymealkit.domain.DeliveryVO">
		select * from delivery where member_id=#{member_id}
	</select>
	

</mapper>